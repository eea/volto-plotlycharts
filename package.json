{
  "name": "@eeacms/volto-plotlycharts",
  "version": "12.0.5",
  "description": "Plotly Charts and Editor integration for Volto",
  "main": "src/index.js",
  "author": "European Environment Agency: IDM2 A-Team",
  "license": "MIT",
  "homepage": "https://github.com/eea/volto-plotlycharts",
  "keywords": [
    "volto-addon",
    "volto-block",
    "volto-plotlychart",
    "volto",
    "plone",
    "react",
    "plotly"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eea/volto-plotlycharts.git"
  },
  "addons": [
    "@eeacms/volto-embed",
    "@eeacms/volto-matomo",
    "@eeacms/volto-datablocks"
  ],
  "dependencies": {
    "@eeacms/react-chart-editor": "0.47.2",
    "@eeacms/volto-datablocks": "*",
    "@eeacms/volto-embed": "*",
    "@eeacms/volto-matomo": "*",
    "@plone/scripts": "*",
    "handsontable": "^15.2.0",
    "jsoneditor": "10.2.0",
    "plotly.js": "^2.35.3",
    "react-plotly.js": "2.6.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@plone/scripts": "*",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-controls": "6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cypress": "13.1.0",
    "dotenv": "^16.3.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "md5": "^2.3.0"
  },
  "resolutions": {
    "moment": "<=2.29.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "make lint-fix",
      "make prettier-fix"
    ],
    "src/**/*.{jsx}": [
      "make i18n"
    ],
    "theme/**/*.{css,less}": [
      "make stylelint-fix"
    ],
    "src/**/*.{css,less}": [
      "make stylelint-fix"
    ],
    "theme/**/*.overrides": [
      "make stylelint-fix"
    ],
    "src/**/*.overrides": [
      "make stylelint-fix"
    ]
  },
  "scripts": {
    "release": "release-it",
    "release-major-beta": "release-it major --preRelease=beta",
    "release-beta": "release-it --preRelease=beta",
    "bootstrap": "npm install -g ejs; npm link ejs; node bootstrap",
    "test": "make test",
    "test:fix": "make test-update",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "yarn stylelint:fix && yarn prettier:fix && yarn lint:fix",
    "stylelint": "make stylelint",
    "stylelint:overrides": "make stylelint-overrides",
    "stylelint:fix": "make stylelint-fix",
    "prettier": "make prettier",
    "prettier:fix": "make prettier-fix",
    "lint": "make lint",
    "lint:fix": "make lint-fix",
    "i18n": "make i18n",
    "cypress:run": "make cypress-run",
    "cypress:open": "make cypress-open",
    "prepare": "husky install"
  }
}